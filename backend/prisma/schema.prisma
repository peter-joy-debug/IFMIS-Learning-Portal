  level                        Int
  message_required             Boolean
  name                         String                         @db.VarChar(255)
  require_editor               Boolean
  status                       String                         @db.VarChar(255)
  action                       String                         @db.Uuid
  parent_id                    String?                        @db.Uuid
  user_action_id               String?                        @db.Uuid
  comments                     String?
  next_action_detail_id        String?                        @db.Uuid
  role_ids                     String?
  budg_budget_call_circular    budg_budget_call_circular[]
  budg_budget_release          budg_budget_release[]
  budg_budget_sheet            budg_budget_sheet[]
  budg_commitment_plan         budg_commitment_plan[]
  budg_commitment_plan_ceiling budg_commitment_plan_ceiling[]
  wf_action_comment            wf_action_comment[]

  @@schema("public")
}

model users {
  id         String   @id
  firstname  String
  lastname   String
  username   String   @unique
  email      String   @unique
  password   String
  department String[]
  role       String
  avatar     String?
  status     String   @default("Active")

  @@schema("user_schema_uts")
}

model faqs {
  id       String @id
  question String
  answer   String

  @@schema("uts")
}

model replies {
  id          String   @id
  ticketId    String
  userId      String
  detail      String
  attachments String[]
  date        DateTime @default(now())
  tickets     tickets  @relation(fields: [ticketId], references: [id])

  @@schema("uts")
}

model tickets {
  id          String    @id
  senderId    String
  subject     String
  detail      String
  assignedTo  String[]
  status      String    @default("Received")
  date        DateTime  @default(now())
  attachments String[]
  department  String[]
  shared      String[]
  visibility  String    @default("Visible")
  replies     replies[]

  @@schema("uts")
}
